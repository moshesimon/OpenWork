{
  "name": "openwork",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest",
    "search:adapters:install": "python3 -m pip install -r agent_runtime/search_adapters/requirements.txt",
    "search:chatindex:api": "python3 -m uvicorn agent_runtime.search_adapters.chatindex_api:app --host 127.0.0.1 --port 8101 --reload",
    "search:pageindex:api": "python3 -m uvicorn agent_runtime.search_adapters.pageindex_api:app --host 127.0.0.1 --port 8102 --reload",
    "search:officeindex:api": "python3 -m uvicorn agent_runtime.search_adapters.officeindex_api:app --host 127.0.0.1 --port 8103 --reload",
    "search:officeindex:reindex:full": "python3 -m agent_runtime.search_adapters.officeindex_reindex --mode full",
    "search:officeindex:reindex:incremental": "python3 -m agent_runtime.search_adapters.officeindex_reindex --mode incremental",
    "search:officeindex:test": "python3 -m unittest agent_runtime.search_adapters.test_officeindex_api",
    "search:adapters:run": "bash agent_runtime/search_adapters/run-local.sh",
    "prisma:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:rebuild": "rm -f prisma/dev.db prisma/dev.db-journal prisma/init.sql && prisma migrate diff --from-empty --to-schema prisma/schema.prisma --script > prisma/init.sql && prisma db execute --file prisma/init.sql",
    "prisma:seed": "prisma db seed",
    "setup": "prisma generate && npm run db:push && prisma db seed"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.44",
    "@ai-sdk/openai": "^3.0.29",
    "@prisma/adapter-better-sqlite3": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "@trigger.dev/sdk": "^4.3.3",
    "@univerjs-pro/exchange-client": "^0.15.5",
    "@univerjs/preset-docs-core": "^0.15.5",
    "@univerjs/preset-sheets-core": "^0.15.5",
    "@univerjs/presets": "^0.15.5",
    "@univerjs/slides": "^0.15.5",
    "@univerjs/slides-ui": "^0.15.5",
    "ai": "^6.0.89",
    "better-sqlite3": "^12.6.2",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^7.4.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
